A = [1024 -2029 1006;
    1024 -2009 988;
    1024 -1970 955;
    1024 -1878 890;
    1024 -1660 772;
    1024 -1115 569;
    1024 141 239];
B = [1024 0 -1024;
    1024 0 -1024;
    1024 0 -1024;
    1024 0 -1024;
    1024 0 -1024;
    1024 0 -1024;
    1024 0 -1024;];

A = A./1024;
B = B./1024;
G = [8, 17, 34, 66, 125, 250, 500, 1000, 2000];
G = G./10

lim = [180 180 180 120 90 60 30];


Hgen = zeros(1024,1)

for i = 1:7,
    Bi = B(i,:);
    Ai = A(i,:);
    [H,W] = freqz(Bi,Ai,1024,8000);
    H = G(i)*H;
    Hgen = Hgen + H;
    H = 10*log(H);
    W = 10*log(W);
    plot(W,H);
    hold all;
end
plot(W,10*log(Hgen))

